{
  "manifest_version": 2,
  "name": "UW AutoCalendar",
  "description": "This extension will integrate UW calendar with Google calendar",
  "version": "1.0",
  "icons": {
    "16": "./images/extension_profile/extension-icon-16.png",
    "32": "./images/extension_profile/extension-icon-32.png",
    "48": "./images/extension_profile/extension-icon-48.png",
    "128": "./images/extension_profile/extension-icon-128.png"
  },
  "oauth2": {
    "client_id": "770480046288-j50h3rqlnm9t3pfmqs1tpfqnc5qt06iv.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/ https://canvas.uw.edu/api; object-src 'self'",
  "options_page": "/html/options.html",
  "browser_action": {
    "default_icon": {
      "16": "./images/extension_profile/extension-icon-16.png",
      "32": "./images/extension_profile/extension-icon-32.png",
      "48": "./images/extension_profile/extension-icon-48.png",
      "128": "./images/extension_profile/extension-icon-128.png"
    },
    "default_popup": "./html/popup.html"
  },
  "permissions": [
    "identity",
    "https://www.googleapis.com/auth/calendar",
    "https://canvas.uw.edu/api/*",
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "js/autocalendar.js"
  ]
}
